{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Ensures babel-plugin-react-compiler, react-compiler-runtime and eslint-plugin-react-hooks all update together",
  "packageRules": [
    {
      "followTag": "latest",
      "groupName": "react-compiler",
      "matchPackageNames": [
        "babel-plugin-react-compiler",
        "eslint-plugin-react-hooks",
        "react-compiler-runtime"
      ]
    }
  ]
}
