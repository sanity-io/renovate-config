{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>sanity-io/renovate-config"],
  "packageRules": [
    {
      "matchDepTypes": ["dependencies", "overrides", "resolutions"],
      "rangeStrategy": "update-lockfile",
      "semanticCommitType": "isBump={{isBump}} isRange={{isRange}} isLockfileUpdate={{isLockfileUpdate}} current={{currentVersion}}=>{{currentValue}} new={{newVersion}}=>{{newValue}} indentation={{indentation}}"
    }
  ]
}
